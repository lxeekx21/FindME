# Database URLs
# Alembic (sync, psycopg2)
DB_URL=postgresql+psycopg2://root@localhost:5432/find_south
# App (async, asyncpg)
DB_URL_ASYNC=postgresql+asyncpg://root@localhost:5432/find_south

# JWT Configuration
JWT_SECRET_KEY=your-secret-key-change-this-in-production-make-it-long-and-random
JWT_ALGORITHM=HS256
JWT_ACCESS_TOKEN_EXPIRE_MINUTES=30
JWT_REFRESH_TOKEN_EXPIRE_DAYS=7

# CORS
CORS_ORIGINS=http://localhost:3000,http://localhost:8000

# Logging
LOG_LEVEL=INFO

# Age progression
AGE_PROGRESSION_SPACES=akhaliq/Photo-to-Older,akhaliq/Face-Aging
AGE_PROGRESSION_SPACE=akhaliq/Face-Aging